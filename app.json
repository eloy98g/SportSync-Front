{
  "expo": {
    "name": "Sport Up",
    "slug": "sport-up",
    "scheme": "sportup",
    "version": "1.0.0",
    "owner": "eloy98g",
    "orientation": "portrait",
    "icon": "./src/assets/metadata/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/metadata/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eloy98g.sportup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/metadata/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eloy98g.sportup",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "@MAP_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/metadata/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a4f84c1-c00e-4016-9de5-d419e272f4d4"
      }
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermission": "Permite a UserApp el acceso a la cámara",
          "enableCodeScanner": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite a UserApp el acceso a tu ubicación"
        }
      ]
    ]
  }
}
